function palindrome()
{
    let x = 'racecar';

    let len = x.length;

    for (let i = 0; i < len / 2; i++)
    {
        if(x[i] !== x[len - 1 - i])
        {
            console.log("Given string is not a palindrome.");
        }
        else
        {
            console.log("Given string is a palindrome");
        }
    }
}

palindrome();



